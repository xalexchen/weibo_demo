<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android1="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android1:clickable="true" >

    <ImageView
        android1:id="@+id/sidemenu_home"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignParentLeft="true"
        android1:layout_below="@+id/sidemenu_bg_profile_intro"
        android1:layout_marginTop="50dp"
        android1:clickable="true"
        android1:src="@drawable/ic_tab_home_nor" />

    <ImageView
        android1:id="@+id/sidemenu_at"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignLeft="@+id/sidemenu_home"
        android1:layout_below="@+id/sidemenu_home"
        android1:src="@drawable/ic_tab_at_nor" />

    <ImageView
        android1:id="@+id/sidemenu_comment"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignRight="@+id/sidemenu_at"
        android1:layout_below="@+id/sidemenu_at"
        android1:src="@drawable/ic_tab_comment_nor" />

    <ImageView
        android1:id="@+id/sidemenu_message"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignRight="@+id/sidemenu_comment"
        android1:layout_below="@+id/sidemenu_comment"
        android1:src="@drawable/ic_tab_message_nor" />

    <ImageView
        android1:id="@+id/sidemenu_info"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignRight="@+id/sidemenu_message"
        android1:layout_below="@+id/sidemenu_message"
        android1:src="@drawable/ic_tab_info_nor" />

    <ImageView
        android1:id="@+id/sidemenu_topic"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignLeft="@+id/sidemenu_info"
        android1:layout_below="@+id/sidemenu_info"
        android1:src="@drawable/ic_tab_topic_nor" />

    <ImageView
        android1:id="@+id/sidemenu_plaza"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignRight="@+id/sidemenu_topic"
        android1:layout_below="@+id/sidemenu_topic"
        android1:src="@drawable/ic_tab_plaza_nor" />

    <ImageView
        android1:id="@+id/sidemenu_setting"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignLeft="@+id/imageView7"
        android1:layout_alignParentBottom="true"
        android1:layout_marginBottom="28dp"
        android1:src="@drawable/ic_tab_setting_nor" />

    <ImageView
        android1:id="@+id/sidemenu_compose"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_above="@+id/sidemenu_setting"
        android1:layout_alignLeft="@+id/sidemenu_setting"
        android1:src="@drawable/ic_tab_compose_nor" />

    <ImageView
        android1:id="@+id/sidemenu_refresh"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_above="@+id/sidemenu_compose"
        android1:layout_alignLeft="@+id/sidemenu_compose"
        android1:src="@drawable/ic_tab_refresh_nor" />

    <ImageView
        android1:id="@+id/sidemenu_bg_profile_intro"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignParentLeft="true"
        android1:layout_alignParentTop="true"
        android1:layout_alignRight="@+id/imageView1"
        android1:layout_marginLeft="5dp"
        android1:layout_marginTop="10dp"
        android1:src="@drawable/bg_profile_intro" />

</RelativeLayout>
